{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}/openocd",
            "executable": "${workspaceFolder}/build/lightkraken2.elf",
            "name": "Debug Main Firmware",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "H563ZIT6",
            "interface": "swd",
            "configFiles": [
                "./stlink-dap.cfg",
                "./stm32h5x.cfg"
            ],
            "openOCDLaunchCommands": ["adapter speed 12000"],
            "preLaunchTask": "STM32 upload",
            "swoConfig": {
                "source": "probe",
                "enabled": true,
                "swoFrequency": 2000000,
                "cpuFrequency": 250000000,
                "decoders": [
                    {
                        "port": 0,
                        "type": "console",
                        "label": "Output",
                        "encoding":"utf8"
                    }
                ]
            }
        }
    ],
}